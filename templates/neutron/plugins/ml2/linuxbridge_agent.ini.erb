[linux_bridge]
physical_interface_mappings = provider:<%=@networking['primary']%>

[vxlan]
enable_vxlan = true
local_ip = <%=@ipaddress_enp0s5f1%>
l2_population = true
udp_dstport = 4789

[securitygroup]
enable_security_group = true
firewall_driver = neutron.agent.linux.iptables_firewall.IptablesFirewallDriver
